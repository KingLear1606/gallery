{"meta":{"title":"MyGallery","subtitle":null,"description":null,"author":"Breeze","url":"https://kinglear1606.github.io/Gallery","root":"/Gallery/"},"pages":[{"title":"编辑器","date":"2025-11-14T13:53:03.632Z","updated":"2025-11-14T13:53:03.632Z","comments":true,"path":"editor.html","permalink":"https://kinglear1606.github.io/Gallery/editor.html","excerpt":"","text":""},{"title":"GridAll","date":"2025-11-14T13:53:03.632Z","updated":"2025-11-14T13:53:03.632Z","comments":true,"path":"grid-all.html","permalink":"https://kinglear1606.github.io/Gallery/grid-all.html","excerpt":"","text":""},{"title":"Grid","date":"2025-11-14T13:53:03.632Z","updated":"2025-11-14T13:53:03.632Z","comments":true,"path":"grid.html","permalink":"https://kinglear1606.github.io/Gallery/grid.html","excerpt":"","text":""},{"title":"Map-照片集","date":"2025-11-14T13:53:03.632Z","updated":"2025-11-14T13:53:03.632Z","comments":true,"path":"location.html","permalink":"https://kinglear1606.github.io/Gallery/location.html","excerpt":"","text":""},{"title":"","date":"2025-11-14T13:53:03.632Z","updated":"2025-11-14T13:53:03.632Z","comments":true,"path":"manifest.json","permalink":"https://kinglear1606.github.io/Gallery/manifest.json","excerpt":"","text":"{\"name\":\"Gallery\",\"short_name\":\"Gallery\",\"start_url\":\".\",\"display\":\"standalone\",\"icons\":[{\"sizes\":\"144x144\",\"type\":\"image/png\"}],\"lang\":\"zh-CN\",\"background_color\":\"#ffffff\"}"},{"title":"Photo","date":"2025-11-14T13:53:03.632Z","updated":"2025-11-14T13:53:03.632Z","comments":true,"path":"photo.html","permalink":"https://kinglear1606.github.io/Gallery/photo.html","excerpt":"","text":""},{"title":"数据查询","date":"2025-11-14T13:53:03.632Z","updated":"2025-11-14T13:53:03.632Z","comments":true,"path":"query.html","permalink":"https://kinglear1606.github.io/Gallery/query.html","excerpt":"","text":""},{"title":"查询用例","date":"2022-04-19T00:00:00.000Z","updated":"2025-11-14T13:53:03.633Z","comments":true,"path":"queryphase.html","permalink":"https://kinglear1606.github.io/Gallery/queryphase.html","excerpt":"","text":"Group all image maker SELECT IFNULL(EXIFData.maker, 'NONE') AS maker, COUNT(*) AS count FROM Photo LEFT OUTER JOIN EXIFData ON Photo.exif_data_id = EXIFData.id GROUP BY maker ORDER BY maker TRY IT Group by focal length and order by count. SELECT EXIFData.focal_length, COUNT(*) AS count FROM Photo LEFT OUTER JOIN EXIFData ON Photo.exif_data_id = EXIFData.id WHERE Photo.exif_data_id IS NOT NULL OR Photo.exif &lt;&gt; '' GROUP BY EXIFData.focal_length ORDER BY CAST(EXIFData.focal_length as DOUBLE) DESC TRY IT Give me random 5 fujifilm picture SELECT photo.*, exifdata.maker FROM photo LEFT OUTER JOIN exifdata ON photo.exif_data_id = exifdata.id WHERE photo.location_id NOT NULL AND exifdata.maker = 'FUJIFILM' ORDER BY RANDOM() LIMIT 5; TRY IT Get N group by F Number SELECT id, path, name, desc, f_number, lo, hi FROM ( SELECT p.id, p.path, p.name, p.desc, e.f_number, l.lo, l.hi, ROW_NUMBER() OVER (PARTITION BY e.f_number ORDER BY p.id) AS row_num FROM photo p JOIN exifdata e ON p.exif_data_id = e.id LEFT JOIN location l ON p.location_id = l.id ) subquery WHERE row_num &lt;= 10; TRY IT Group by country SELECT country, COUNT(*) as count FROM location GROUP BY country TRY IT"},{"title":"随机照片","date":"2025-11-14T13:53:03.633Z","updated":"2025-11-14T13:53:03.633Z","comments":true,"path":"random.html","permalink":"https://kinglear1606.github.io/Gallery/random.html","excerpt":"","text":""},{"title":"Gallery","date":"2025-11-14T13:53:03.633Z","updated":"2025-11-14T13:53:03.633Z","comments":true,"path":"rss.html","permalink":"https://kinglear1606.github.io/Gallery/rss.html","excerpt":"","text":""},{"title":"状态监控","date":"2025-11-14T13:53:03.633Z","updated":"2025-11-14T13:53:03.633Z","comments":true,"path":"status.html","permalink":"https://kinglear1606.github.io/Gallery/status.html","excerpt":"","text":""},{"title":"年终总结","date":"2025-11-14T13:53:03.633Z","updated":"2025-11-14T13:53:03.633Z","comments"